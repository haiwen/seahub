#: seahub/templates/sysadmin/sys_useradmin_admins.html:7
#: seahub/templates/sysadmin/sys_useradmin_ldap.html:8
#: seahub/templates/sysadmin/sys_useradmin_paid.html:7
msgid "Search users..."
msgstr "Przeszukaj użytkowników..."

#: seahub/institutions/templates/institutions/info.html:7
#: seahub/templates/js/sysadmin-templates.html:8
#: seahub/templates/js/sysadmin-templates.html:98
#: seahub/templates/sysadmin/base.html:16
msgid "Info"
msgstr "Informacje"

#: seahub/institutions/templates/institutions/info.html:13
#: seahub/templates/home_base.html:59
#: seahub/templates/js/sysadmin-templates.html:19
#: seahub/templates/js/sysadmin-templates.html:121
#: seahub/templates/js/sysadmin-templates.html:753
#: seahub/templates/js/templates.html:1170
#: seahub/templates/js/templates.html:2056 seahub/templates/libraries.html:5
#: seahub/templates/sysadmin/base.html:27
